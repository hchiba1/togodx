#!/usr/bin/env spang2
# @endpoint https://orth.dbcls.jp/sparql-test
# @option --time -a

PREFIX cao: <http://chip-atlas.org/ontology/>

SELECT DISTINCT ?tf ?tf2 (COUNT(?s) AS ?count)
WHERE {
  ?s a cao:Colocalization ;
     cao:hasTF ?tf ;
     cao:hasPartnerTF ?tf2 ;
     cao:coloScore ?score .
   FILTER (?score > 0)
}
ORDER BY DESC(?count)
